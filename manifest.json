{
  "name": "Tab Manager",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A simple manager for quickly finding and opening tabs.",
  "options_page": "options.html",
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "options.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": { "default": "Ctrl+Shift+4" }
    }
  }  
}